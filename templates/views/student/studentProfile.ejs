<%- include('../includes/studentheader.ejs')%>

        <div class="container-fluid rounded bg-white mt-5 mb-5 ml-0 mr-0">
        <div class="row">
            <div class="col-md-3 mt-0 border-right">
                <div class="d-flex flex-column align-items-center text-center p-3 py-5"><img class="rounded-circle "
                        width="150px"
                        src="https://st3.depositphotos.com/15648834/17930/v/600/depositphotos_179308454-stock-illustration-unknown-person-silhouette-glasses-profile.jpg">
                    <button type="button"
                        onclick="window.location.href=`link for your material`"
                        class="btn btn-outline-dark m-2 btn-block">Material Section</button>
                    <button type="button" onclick="window.location.href='/student/companysection';"
                        class="btn btn-outline-dark m-2 btn-block">Company Section</button>
                    <button type="button" onclick="window.location.href='/student/QandA';"
                        class="btn btn-outline-dark m-2 btn-block">Question Section</button>
                    <button type="button" onclick="window.location.href='/student/seeAcceptedCompany';"
                        class="btn btn-outline-dark m-2 btn-block">See Accepted Company</button>
                    <button type="button" onclick="window.location.href='/student/studentProfile';"
                        class="btn btn-outline-dark m-2 btn-block">Profile</button>
                </div>
            </div>
            <div class="col-md-5 border-right mt-5 ">
                <div class="card-block ml-4">
                    <h4 class="m-b-20 p-b-5 b-b-default mb-4 f-w-600 text-uppercase font-weight-bold">Information</h4>
                    <div class="row mb-2">
                        <div class="col-sm-6 mb-2">
                            <h6 class="text-muted  f-w-400">Name</h6>
                            <p class="m-b-10 mb-2 font-weight-bold f-w-600">
                                <%=stdDetails.sname%>
                            </p>
                        </div>
                        <div class="col-sm-6 mb-2">
                            <h6 class="text-muted  f-w-400">ID</h6>
                            <p class="m-b-10 mb-2 font-weight-bold f-w-600">
                                <%=stdDetails.sid%>
                            </p>
                        </div>
                    </div>
                    <div class="row mb-2">
                        <div class="col-sm-6 mb-2">
                            <h6 class="text-muted  f-w-400">Mobile Number</h6>
                            <p class="m-b-10 mb-2 font-weight-bold f-w-600">
                                <%=stdDetails.mobileNo%>
                            </p>
                        </div>
                        <div class="col-sm-6 mb-2">
                            <h6 class="text-muted  f-w-400">Email Id</h6>
                            <p class="m-b-10 mb-2 font-weight-bold f-w-600">
                                <%=stdDetails.email%>
                            </p>
                        </div>
                    </div>
                    <div class="row mb-2">
                        <div class="col-sm-6 mb-2">
                            <h6 class="text-muted  f-w-400">Branch</h6>
                            <p class="m-b-10 mb-2 font-weight-bold f-w-600">
                                <%=stdDetails.branch%>
                            </p>
                        </div>

                        <div class="col-sm-6 mb-2">
                            <h6 class="text-muted  f-w-400">CGPA</h6>
                            <p class="m-b-10 mb-2 font-weight-bold f-w-600">
                                <%=stdDetails.cgpa%>
                            </p>
                        </div>
                    </div>
                    <div class="row mb-2">
                        <div class="col-sm-6 mb-2">
                            <h6 class="text-muted  f-w-400">Interested Area</h6>
                            <p class="m-b-10 mb-2 font-weight-bold f-w-600">
                                <%=stdDetails.jobType%>
                            </p>
                        </div>

                        <div class="col-sm-6 mb-2">
                            <h6 class="text-muted  f-w-400">Address</h6>
                            <p class="m-b-10 mb-2 font-weight-bold f-w-600">
                                <%=stdDetails.address%>
                            </p>
                        </div>
                    </div>
                    <div class="row mb-2">
                        <h6 class="text-muted  f-w-400">Description</h6>
                        <p class="m-b-10 mb-2 font-weight-bold f-w-600">
                            <%=stdDetails.description%>
                        </p>


                        <h6 class="text-muted  f-w-400">Resume Link</h6>
                        <a href="<%=stdDetails.resume%>">Click To See Your Resume</a>
                        <button type="button" class="btn btn-outline-dark"
                            onclick="window.location.href='/student/editDetails'">Edit</button>
                    </div>
                </div>
            </div>
            <div class="col-md-4 ">
                <div id="carouselExampleIndicators" class="carousel slide carousel-fade" data-interval="3000" data-ride="carousel" style="height: 40em; width: 100%;">
                    <ol class="carousel-indicators">
                      <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                      <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                      <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                    </ol>
                    <div class="carousel-inner">
                      <div class="carousel-item active">
                        <img class="d-block w-100" style="height: 100%; width: 90%;" src="https://th.bing.com/th/id/R.b78dadb76aa7c02bc9745b494b7abc97?rik=i%2bzlpZXKwlPgFw&riu=http%3a%2f%2fbuzz16.com%2fwp-content%2fuploads%2f2016%2f10%2finspirational-quotes-for-college-students-34.jpg&ehk=p4hAXDDmrRiNLVXeBGU5tVkAGGvsYiaNhhTKT9dUgJs%3d&risl=&pid=ImgRaw&r=0" alt="First slide">
                      </div>
                      <div class="carousel-item">
                        <img class="d-block w-100 mb-0" style="height: 100%; width: 90%;" src=https://th.bing.com/th/id/R.fe1e69649408225d5bfa05091d626213?rik=CCJTgzOS3fkfnA&riu=http%3a%2f%2f2.bp.blogspot.com%2f-HOyZZ0qwD2c%2fUVBAjm6ZlVI%2fAAAAAAAAHWQ%2fdE-zmDteOrs%2fs1600%2fthesecret.jpg&ehk=ixOFZDjd%2bC6Q7Gi56VpWL63LX2Q5839CKR%2fn0joj%2faE%3d&risl=&pid=ImgRaw&r=0" alt="Second slide">
                      </div>
                      <div class="carousel-item">
                        <img class="d-block w-100"  style="height: 100%; width: 90%;" src="https://th.bing.com/th/id/OIP.EMw0_cfa8Sy_ZKiKTTE1DgHaLH?pid=ImgDet&rs=1" alt="Third slide">
                      </div>
                    </div>
                    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                      <span class="sr-only">Previous</span>
                    </a>
                    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                      <span class="carousel-control-next-icon" aria-hidden="true"></span>
                      <span class="sr-only">Next</span>
                    </a>
                  </div>

            </div>
        </div>
    </div>
    </div>
    </div>
    

    <%- include('../includes/studentfooter.ejs')%>
